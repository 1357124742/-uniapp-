<view><block wx:for="{{publish_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-card case',isCard?'no-card':'']}}"><view class="{{['cu-card dynamic',isCard?'no-card':'']}}"><view class="cu-item shadow"><view class="cu-list menu-avatar"><view class="cu-item"><view class="cu-avatar round lg"><image style="border-radius:50%;" src="{{item.avatarUrl}}" mode="widthFix"></image></view><view class="content flex-sub"><view>{{''+item.nickName+''}}</view><view class="text-gray text-sm flex justify-between">{{''+item.show1+"   "+item.show2+"    "+item.show3+''}}</view></view><view class="tiezi" data-index="{{index}}" data-event-opts="{{[['tap',[['delete_tiezi',['$event']]]]]}}" bindtap="__e"><image class="delete" src="../../static/删除分类.png" data-index="{{index}}"></image></view></view></view><view class="text-content" style="word-break:break-all;">{{''+item.describe+''}}</view><view class=" flex-sub padding-lr"><swiper class="swiper" indicator-dots="true" circular="true"><block wx:for="{{item.img}}" wx:for-item="image" wx:for-index="__i0__" wx:key="*this"><swiper-item><image style="height:100%;" src="{{image}}" data-url="{{image}}" data-index="{{index}}" data-event-opts="{{[['tap',[['viewimage',['$event']]]]]}}" bindtap="__e"></image></swiper-item></block></swiper></view><view class="text-gray text-sm text-right padding"><block wx:if="{{checked[index]==1}}"><view class="image_view" data-index="{{index}}" data-event-opts="{{[['tap',[['Dian_Zan',['$event']]]]]}}" bindtap="__e"><image class="like_image" src="../../static/like_checked.png"></image><view class="image_title">{{item.like_number}}</view></view></block><block wx:else><view class="image_view" data-index="{{index}}" data-event-opts="{{[['tap',[['Dian_Zan',['$event']]]]]}}" bindtap="__e"><image class="like_image" src="../../static/like.png"></image><view class="image_title">{{item.like_number}}</view></view></block></view></view></view></view></block></view>